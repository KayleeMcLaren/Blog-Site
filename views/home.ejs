//Display header on "home" page
<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= startingContent %> </p>

//Display blog posts
  <%  posts.forEach(function(post){ %>

    <h1><%=post.title%></h1>
    <p>
    <%=post.content.substring(0, 100) + " ..."%>  //Limit the number of characters displayed to 100
    <a href="/posts/<%=post.title%>">Read More</a>  
    </p>


    <% }) %>

//Display footer on "home" page
<%- include("partials/footer"); -%>
